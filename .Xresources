URxvt.scrollBar: false
URxvt*termName: xterm-256color

! special
*.foreground:   #c5c8c6
*.background:   #252526
*.cursorColor:  #c5c8c6

! black
*.color0:       #2d2d2d
*.color8:       #747369

! red
*.color1:       #f2777a
*.color9:       #f2777a

! green
*.color2:       #99cc99
*.color10:      #99cc99

! yellow
*.color3:       #ffcc66
*.color11:      #ffcc66

! blue
*.color4:       #6699cc
*.color12:      #6699cc

! magenta
*.color5:       #cc99cc
*.color13:      #cc99cc

! cyan
*.color6:       #66cccc
*.color14:      #66cccc

! white
*.color7:       #c5c8c6
*.color15:      #f2f0ec

! man page coloring
*.colorIT:		#87af5f
*.colorBD:		#d7d7d7
*.colorUL:		#87afd7

*.font: xft:Terminus:style=Regular:size=14
*.boldFont: xft:Terminus:style=Bold:size=14

*.letterSpace: 1
